        var originHost="http://localhost:38183"; //"https://tadvisorchatbottest.herokuapp.com/";
        var isInIframe = window.frameElement && window.frameElement.nodeName == "IFRAME";

        if(isInIframe){

            targetOrigin=originHost; 
        }
        else{
            targetOrigin="https://tadvisorchatbottest.herokuapp.com/";
        }
        var myClientDataJS=JSON.stringify({"clientData":{"name": "Juan", "age": 30}});

        myClientDataJS=JSON.parse(JSON.stringify(myClientDataJS)); 

        var myServerDataJS;
        var serverEvent=window.parent;

        function sendGetData(event, callback){
            serverEvent.postMessage(myClientDataJS, originHost);
            callback();
        }
        
        window.addEventListener('message', function(event){

            if(event.origin!==originHost)
