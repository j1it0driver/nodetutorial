<body onload="send_event('custom_event','Guest')" class="bodyBckg"> <!-- "send_event('start_event','test')send_event('custom_event','Jaime')" -->
    <div class="container">
        <div class="row border-0">
            <div class="col-12 pt-3">
                <div class="media">
                    <img class="d-flex align-self-top mr-5 rounded-circle" src="/images/avatartadvisor0.png" alt="Generic placeholder image" height="50">
                    <div class="media-body">
                        <h5 class="mt-0">T-Advisor</h5>
                        <div class="status">
                            <label class="statusToUser" id="statusMessages">Let's start a conversation...</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row border-0">
            <div class="chatBckg col-12 px-1">

                <div class="scrollable chat-messages p-1" id="chatHistory">
                </div>

            </div>
        </div>
        <div class="row border-0">
            <div class=" bodyBckg col-12">

                <div class="row d-flex justify-content-between mt-3" >
                    <div class="col-9 ">
                        <div class="textInputContainer form-group ">
                           <input id="speech" type="text" autofocus placeholder="Type here... + Enter" class="form-control">
                       </div>
                    </div>
                    <div class="col-3 d-flex justify-content-end pl-0 pr-3">
                        <button class="btn btn-primary ml-auto align-self-start p-1 col-12 col-sm-8 col-md-6" id="rec"><img src="/images/mic.png" alt="" ><span id="rec1"></span></button>
                    </div>

                </div>

            </div>
        </div>
    </div>



